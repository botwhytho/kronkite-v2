<div>
<% function truncate(string, slicePt) {
    if(!slicePt) {
        return `${string.slice(0, 24)}...`
    } else {
        return string.slice(0, string.lastIndexOf(":"));
    }
   
} %>
    <header class="header header-6 header-custom">
    <span class="clr-icon-return-to-home">
        <a href="#/"><clr-icon shape="circle-arrow left" class="clr-icon-circle-arrow-left"></a>
        </clr-icon>
    </span>
    <div class="video-marquee" data-title="">
    	<%=data["snippet"]["title"]%>
    </span>
    </header>
    <div class="content-container">
        <div class="content-area video-view">
        	<div> </div>
        	<div style="border-bottom: 5px dotted black; margin: 10px 55px 10px 0px;"></div>
			<section>
                <div><img src="<%=data["thumbnails"]["medium"]["url"]%>"></div>
            </section>
        </div>
    </div>
</div>