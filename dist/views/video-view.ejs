<div>
<% function truncate(string, slicePt) {
    if(!slicePt) {
        return `${string.slice(0, 24)}...`
    } else {
        return string.slice(0, string.lastIndexOf(":"));
    }
} %>
    <header class="header header-6 header-custom">
    <span class="clr-icon-return-to-home">
        <a href="#/videos"><section shape="circle-arrow left" class="clr-icon-circle-arrow-left">
            <svg version="1.1" viewBox="0 0 36 36" preserveAspectRatio="xMidYMid meet" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                <title>circle-arrow-line</title>
                <path d="M18,2A16,16,0,1,0,34,18,16,16,0,0,0,18,2Zm0,30A14,14,0,1,1,32,18,14,14,0,0,1,18,32Z" class="clr-i-outline clr-i-outline-path-1"></path><path d="M18.08,8.26l-7.61,7.61a1,1,0,1,0,1.41,1.41L17,12.18v15a1,1,0,0,0,2,0V12l5.28,5.28a1,1,0,1,0,1.41-1.41Z" class="clr-i-outline clr-i-outline-path-2"></path>
            </svg>
        </section></a>
    </span>
    <div class="video-marquee" data-title="">
    	<%=data["snippet"]["title"]%>
    </span>
    </header>
    <div class="content-container">
        <div class="content-area content-area-custom video-view">
        	<div class="row row-custom">
			    <div class="col-lg-6 col-md-12 col-sm-12 col-xs-12">
			        <a href="..." class="card">
			            <div class="card-img">
			             <iframe src="https://www.youtube.com/embed/<%=data["id"]%>" width="320" height="180" frameborder="0" allowfullscreen></iframe>
			            </div>
			            <div class="card-block">
			                <p class="card-text">
			                    <%=data["snippet"]["description"]%>
			                </p>
			            </div>
			        </a>
			    </div>
			</div>
        	<div style="border-bottom: 5px dotted black; margin: 10px 55px 10px 0px;"></div>
		</div>
    </div>
</div>